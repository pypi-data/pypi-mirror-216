#id:NXQ_09620
#title:Check there are no entries which are PE2-PE4 with a crosslink annotation with GOLD experimental evidence which is NOT mass spectrometry from a source other than UniProt
#tags:QC,protein existence,PE
#acs:
#count:0
#time:8.416

select distinct ?entry where {
  ?entry a :Entry.
  ?entry :existence ?pe.
  filter ( ?pe not in (:Evidence_at_protein_level,:Uncertain ))
  ?entry :isoform ?iso.
  ?iso :crossLink /:evidence ?ev.
  ?ev :assignedBy ?src.
  filter ( ?src not in (source:Uniprot))
  ?ev :quality :GOLD.
  ?ev :evidenceCode ?eco.
  filter (?eco not in (cv:ECO_0001096 )) # mass spectrometry
  ?eco :childOf cv:ECO_0000006. # experimental evidence
}
