{
    "files.exclude": {
        "**/*.bak": true,
        "**/__pycache__": true,
        "radstar/jetapp/yarn-error.log": true
    },
    "files.trimTrailingWhitespace": true,
    "python.defaultInterpreterPath": "/rs/env/bin/python",
    "python.analysis.extraPaths": [
        "/rs/radstar",
        "/rs/project/repos/*"
    ],
    "python.linting.enabled": true,
    "python.linting.pylintArgs": [
        "--rcfile=/rs/radstar/.pylintrc"
    ],
    "python.linting.pylintEnabled": true,
    "python.linting.pylintPath": "/rs/env/bin/pylint",
    "search.exclude": {
        "radstar/jetapp/webroot": true
    },
    "search.useIgnoreFiles": false
}
