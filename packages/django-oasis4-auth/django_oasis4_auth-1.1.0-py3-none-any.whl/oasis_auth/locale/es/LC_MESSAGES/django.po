msgid "Not controlled exception error."
msgstr "Error de excepción no controlada."

#: oasis_auth/api/services/oasis_auth.py:153
msgid "Error loading email templates."
msgstr "Error cargando las plantillas de correo."

#: oasis_auth/api/services/oasis_auth.py:155
msgid "Email is not registered or does not match."
msgstr "El correo no se encuentra registrado."

#: oasis_auth/api/services/oasis_auth.py:184
msgid "The old password and new password cannot be the same."
msgstr "El password antiguo y el nuevo no coinciden."

#: oasis_auth/api/services/oasis_auth.py:186
msgid "User old password does not match."
msgstr "El password antiguo no coincide."

#: oasis_auth/api/services/oasis_auth.py:188
msgid "User is not authenticated."
msgstr "Usuario no autenticado."

#: oasis_auth/api/services/oasis_auth.py:218
msgid "The code does not match."
msgstr "El código enviado no coincide."

#: oasis_auth/api/services/oasis_auth.py:220
msgid "The code has expired. Please request a new authorization code."
msgstr ""
"El código ha expirado. Por favor solicite un nuevo código de autorización."

#: oasis_auth/api/services/oasis_auth.py:272
#: oasis_auth/api/services/oasis_auth.py:280
msgid "Email or Password incorrect."
msgstr "Correo o clave incorrectos."

#: oasis_auth/lib/utils/auth_actions.py:39
msgid "Invalid initialization data class."
msgstr "Inicialización de clase inválida."

#: oasis_auth/lib/utils/auth_actions.py:51
msgid "Action does not have an associated method."
msgstr "La acción no tiene un método asociado."

#: oasis_auth/models.py:32
msgid "The document id is invalid."
msgstr "El documento es inválido."

#: oasis_auth/tasks/sync_users.py:52
msgid "Error at sync users."
msgstr "Error al sincronizar usuarios."

#: oasis_auth/tasks/sync_users.py:57
msgid "Error saving user profile."
msgstr "Error grabando el perfil del usuario."

